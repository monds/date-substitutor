{
  "partitionYmd": "${date(yyyyMMdd)}",
  "baseDateShort": "${date(yyyyMMdd)}",
  "baseDate": "${date(yyyy-MM-dd)}",
  "baseDateTime": "${date(yyyy-MM-dd HH:mm:ss).truncatedTo(DAYS)}",
  "baseYear": "${date(yyyy)}",
  "pre30mDateShort": "${date(yyyyMMdd).truncatedTo(MINUTES, 30).minus(MINUTES, 30)}",
  "pre30mStartHHmmShort": "${date(HHmm).truncatedTo(MINUTES, 30).minus(MINUTES, 30)}",
  "pre30mStartDateTimeShort": "${date(yyyyMMddHHmmss).truncatedTo(MINUTES, 30).minus(MINUTES, 30)}",
  "pre30mEndDateTimeShort": "${date(yyyyMMddHHmmss).truncatedTo(MINUTES, 30)}",
  "pre30mStartDateTime": "${date(yyyy-MM-dd HH:mm:ss).truncatedTo(MINUTES, 30).minus(MINUTES, 30)}",
  "pre30mEndDateTime": "${date(yyyy-MM-dd HH:mm:ss).truncatedTo(MINUTES, 30)}",
  "minus6DaysDate": "${date(yyyy-MM-dd).minus(DAYS, 6)}",
  "minus6DaysDateShort": "${date(yyyyMMdd).minus(DAYS, 6)}",
  "minus6DaysDateTimeShort": "${date(yyyyMMddHHmmss).truncatedTo(DAYS).minus(DAYS, 6)}",
  "startDateTimeShort": "${date(yyyyMMddHHmmss).truncatedTo(DAYS)}",
  "endDateTimeShort": "${date(yyyyMMddHHmmss).with(MAX)}",
  "startDateTime": "${date(yyyy-MM-dd HH:mm:ss).truncatedTo(DAYS)}",
  "endDateTime": "${date(yyyy-MM-dd HH:mm:ss).with(MAX)}",
  "monthStartDate": "${date(yyyy-MM-dd).withDayOfMonth(1)}",
  "monthStartDateShort": "${date(yyyyMMdd).withDayOfMonth(1)}",
  "monthStartDateTimeShort": "${date(yyyyMMddHHmmss).truncatedTo(DAYS).withDayOfMonth(1)}",
  "minus1YearDateShort": "${date(yyyyMMdd).minus(YEARS, 1)}",
  "minus3MonthsDateShort": "${date(yyyyMMdd).minus(MONTHS, 3)}",
  "minus6MonthsDateShort": "${date(yyyyMMdd).minus(MONTHS, 6)}",
  "pre15mDateShort": "${date(yyyyMMdd).truncatedTo(MINUTES, 15).minus(MINUTES, 15)}",
  "pre15mStartHHmmShort": "${date(HHmm).truncatedTo(MINUTES, 15).minus(MINUTES, 15)}",
  "pre15mStartDateTimeShort": "${date(yyyyMMddHHmmss).truncatedTo(MINUTES, 15).minus(MINUTES, 15)}",
  "pre15mEndDateTimeShort": "${date(yyyyMMddHHmmss).truncatedTo(MINUTES, 15)}",
  "pre15mStartDateTime": "${date(yyyy-MM-dd HH:mm:ss).truncatedTo(MINUTES, 15).minus(MINUTES, 15)}",
  "pre15mEndDateTime": "${date(yyyy-MM-dd HH:mm:ss).truncatedTo(MINUTES, 15)}",
  "pre15mDayStartDateTime": "${date(yyyy-MM-dd HH:mm:ss).minus(MINUTES, 15).truncatedTo(DAYS)}"
}